<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="apis.hsleiden.nl" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <non-jta-data-source>java:comp/env/jdbc/apis</non-jta-data-source>
    <class>nl.hsleiden.webapi.model.Employee</class>
    <class>nl.hsleiden.webapi.model.Student</class>
    <class>nl.hsleiden.webapi.model.Employees</class>
    <class>nl.hsleiden.webapi.model.Students</class>
    <class>nl.hsleiden.webapi.model.Images</class>
    <class>nl.hsleiden.webapi.model.UpdatedGradeStudent</class>
    <class>nl.hsleiden.webapi.model.Person</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
  </persistence-unit>
</persistence>
